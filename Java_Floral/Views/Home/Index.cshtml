<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ptitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PCategory)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pshortdis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plongdis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PImage)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Ptitle)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PCategory.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pshortdis)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Plongdis)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td>
                    <img src="@item.PImage" width="40px" style="border-radius:25%" />
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

