@{
    var totalPage = ViewBag.totalPages;
    var curentPage = ViewBag.cPage;


            //<div>@totalPage</div>




    <ul class="pagination">

        @if (curentPage == 0)
        {

            <li class="page-item "><a class="page-link" asp-route-currentPage="1" asp-action="Index" asp-controller="Home">Previous</a></li>
        }
        else
        {

            <li class="page-item "><a class="page-link" asp-route-currentPage="@(curentPage-1)" asp-action="Index" asp-controller="Home">Previous</a></li>
        }



        <li class="page-item "><a class="page-link" asp-route-currentPage="1" asp-action="Index" asp-controller="Home">First</a></li>

        @for (var i = 1; i <= totalPage; i++)
        {
            if (curentPage == i)
            {
                <li class="page-item active"><a class="page-link" asp-route-currentPage="@i" asp-action="Index" asp-controller="Home">@i</a></li>
            }
            else
            {
                <li class="page-item"><a class="page-link" asp-route-currentPage="@i" asp-action="Index" asp-controller="Home">@i</a></li>
            }
        }
        <li class="page-item "><a class="page-link" asp-route-currentPage="@totalPage" asp-action="Index" asp-controller="Home">Last</a></li>
        @if (curentPage > totalPage)
        {

            <li class="page-item"><a class="page-link" asp-route-currentPage="@totalPage" asp-action="Index" asp-controller="Home">Next</a></li>
        }
        else
        {
            <li class="page-item"><a class="page-link" asp-route-currentPage="@(curentPage+1)" asp-action="Index" asp-controller="Home">Next</a></li>

        }

    </ul>
}

